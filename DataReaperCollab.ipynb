        "        \"nucleigorailgun\",\n",
        "        \"passwd\",\n",
        "        \"payload\",\n",
        "        \"pt/\",\n",
        "        \"qakbot\",\n",
        "        \"qbot\",\n",
        "        \"ransom\",\n",
        "        \"ransomware\",\n",
        "        \"ratel\",\n",
