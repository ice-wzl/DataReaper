        "def do_query(api, query):\n",
        "    results = []\n",
        "    try:\n",
        "        query = \"\".join(query)\n",
        "        print(\"Query {}\".format(query))\n",
        "        result = api.search(query)\n",
        "        for service in result[\"matches\"]:\n",
        "          results.append(service[\"ip_str\"])\n",
        "    except shodan.exception.APIError as e:\n",
        "        print(\"Invalid Shodan API Key...\")\n",
        "        sys.exit(2)\n",
        "    return results\n",
        "\n",
